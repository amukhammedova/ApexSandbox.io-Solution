public List<Integer> fibonacciSeries(Integer n) {

    //code here

    list<Integer> f = new list<Integer>();

    Integer n1=0;

    Integer n2=1;

    Integer n3=1;

    for(Integer i=1;i<=n;i++)

    {

        if(i==1)

        {

            f.add(1);

        }

        else

        {

            n3 = n1 + n2;

            n1 = n2;

            n2 = n3;

            f.add(n3);

        }

    }

    return f;
}