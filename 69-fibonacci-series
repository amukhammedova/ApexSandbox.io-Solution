public List<Integer> fibonacciSeries(Integer n) {
    List<Integer> result = new List<Integer>();
    if (n >= 1) {
        result.add(1);
    }
    if (n >= 2) {
        result.add(1);
    } 
    for (Integer i = 2; i < n; i++) {
        int nextFibonacci = result[i - 1] + result[i - 2];
        result.add(nextFibonacci);
    }
    
    return result;
}