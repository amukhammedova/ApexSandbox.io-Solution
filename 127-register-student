public String registerStudent(String name, String phone, String email) {
    apxio__Student__c student = new apxio__Student__c();
    student.name = name;
    student.apxio__Phone__c = phone;
    student.apxio__Email__c = email;
    return Id;

    apxio__Registration_Number__c regNumber = [Select apxio__Registration_Number__c from apxio__Student__c where id in :student.Id];
    return regNumber.apxio__Registration_Number__c;
}