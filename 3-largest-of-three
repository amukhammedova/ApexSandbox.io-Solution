public static Integer findLargest(Integer num1, Integer num2, Integer num3) {
                                            9           3               5
    if(num1 > num2 > num3){
        return num1;
    }else if(num1 < num2 < num3){
        return num2;
    }else if(num1 > num2 > num3){
        return num1;
    }
}
