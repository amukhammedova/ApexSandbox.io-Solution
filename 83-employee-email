public String employeeEmail(Map<String, String> employeeIdToEmail, String employeeId) {
    if(employeeIdToEmail != null && employeeId != null){
        for(Id i : employeeId){
            if(employeeIdToEmail.contains(i)){
                return employeeIdToEmail.get(i);
            }else{
                return 'info@apexsandbox.io';
            }
        }
    }
}